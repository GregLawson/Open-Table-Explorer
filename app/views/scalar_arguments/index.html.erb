<h1>Listing scalar_arguments</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Value</th>
    <th>Formula</th>
    <th>Ruby type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @scalar_arguments.each do |scalar_argument| %>
  <tr>
    <td><%= scalar_argument.id %></td>
    <td><%= scalar_argument.name %></td>
    <td><%= scalar_argument.value %></td>
    <td><%= scalar_argument.formula %></td>
    <td><%= scalar_argument.ruby_type %></td>
    <td><%= link_to 'Show', scalar_argument %></td>
    <td><%= link_to 'Edit', edit_scalar_argument_path(scalar_argument) %></td>
    <td><%= link_to 'Destroy', scalar_argument, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

