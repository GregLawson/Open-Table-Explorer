<h1>Listing parse_specs</h1>

<table>
  <tr>
    <th>Parse interface</th>
    <th>Table selection</th>
    <th>Row selection</th>
    <th>Name prefix</th>
    <th>Column name location</th>
    <th>Column name selection</th>
    <th>Column selection</th>
    <th>Column value selection</th>
    <th>Table selection is regexp</th>
    <th>Acquisition stream spec</th>
    <th>Id</th>
  </tr>

<% for parse_spec in @parse_specs %>
  <tr>
    <td><%=h parse_spec.parse_interface %></td>
    <td><%=h parse_spec.table_selection %></td>
    <td><%=h parse_spec.row_selection %></td>
    <td><%=h parse_spec.name_prefix %></td>
    <td><%=h parse_spec.column_name_location %></td>
    <td><%=h parse_spec.column_name_selection %></td>
    <td><%=h parse_spec.column_selection %></td>
    <td><%=h parse_spec.column_value_selection %></td>
    <td><%=h parse_spec.table_selection_is_regexp %></td>
    <td><%=h parse_spec.acquisition_stream_spec_id %></td>
    <td><%=h parse_spec.id %></td>
    <td><%= link_to 'Show', parse_spec %></td>
    <td><%= link_to 'Edit', edit_parse_spec_path(parse_spec) %></td>
    <td><%= link_to 'Destroy', parse_spec, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New parse_spec', new_parse_spec_path %>
