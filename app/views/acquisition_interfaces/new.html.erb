<h1>New acquisition_interface</h1>

<% form_for(@acquisition_interface) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :library %><br />
    <%= f.text_area :library %>
  </div>
  <div class="field">
    <%= f.label :acquire_data %><br />
    <%= f.text_area :acquire_data %>
  </div>
  <div class="field">
    <%= f.submit "Create" %>
  </div>
<% end %>

<%= link_to 'Back', acquisition_interfaces_path %>
