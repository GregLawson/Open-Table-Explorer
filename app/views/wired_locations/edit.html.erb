<h1>Editing wired_location</h1>

<% form_for(@wired_location) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :node %><br />
    <%= f.text_field :node %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :parent_node %><br />
    <%= f.text_field :parent_node %>
  </p>
  <p>
    <%= f.label :parent_description %><br />
    <%= f.text_field :parent_description %>
  </p>
  <p>
    <%= f.label :room %><br />
    <%= f.text_field :room %>
  </p>
  <p>
    <%= f.label :wall %><br />
    <%= f.text_field :wall %>
  </p>
  <p>
    <%= f.label :type %><br />
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :ground_test_light %><br />
    <%= f.check_box :ground_test_light %>
  </p>
  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @wired_location %> |
<%= link_to 'Back', wired_locations_path %>
