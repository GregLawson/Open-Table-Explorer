<h1>Listing acquisition_stream_specs</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Acquisition interface</th>
    <th>Url</th>
    <th>Table spec</th>
    <th>Required order</th>
  </tr>

<% for acquisition_stream_spec in @acquisition_stream_specs %>
  <tr>
    <td><%=h acquisition_stream_spec.id %></td>
    <td><%=h acquisition_stream_spec.acquisition_interface %></td>
    <td><%=h acquisition_stream_spec.url %></td>
    <td><%=h acquisition_stream_spec.table_spec_id %></td>
    <td><%=h acquisition_stream_spec.required_order %></td>
    <td><%= link_to 'Show', acquisition_stream_spec %></td>
    <td><%= link_to 'Edit', edit_acquisition_stream_spec_path(acquisition_stream_spec) %></td>
    <td><%= link_to 'Destroy', acquisition_stream_spec, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

