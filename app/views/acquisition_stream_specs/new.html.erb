 <%= form_for(@acquisition_stream_spec) do |f| %>
   f.error_messages 

  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label :acquisition_interface %><br />
	<%= collection_select(:acquisition_stream_spec,:acquisition_interface_id,AcquisitionInterface.all, :id, :name) %>
