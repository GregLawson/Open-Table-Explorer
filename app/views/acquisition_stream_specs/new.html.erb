  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :table_spec_id %><br />
    <%= f.text_field :table_spec_id %>
  </p>
  <p>
    <%= f.label :required_order %><br />
    <%= f.text_field :required_order %>
  </p>
