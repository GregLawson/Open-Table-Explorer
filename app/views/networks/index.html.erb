<h1>Listing networks</h1>

<table>
  <tr>
    <th>Nmap addresses</th>
    <th>Last scan</th>
    <th>Nmap exection time</th>
    <th>Expanded</th>
    <th>Nmap execution time</th>
  </tr>

<% for network in @networks %>
  <tr>
    <td><%=h network.nmap_addresses %></td>
    <td><%=h network.last_scan %></td>
    <td><%=h network.nmap_exection_time %></td>
    <td><%=h network.expanded %></td>
    <td><%=h network.nmap_execution_time %></td>
    <td><%= link_to 'Show', network %></td>
    <td><%= link_to 'Edit', edit_network_path(network) %></td>
    <td><%= link_to 'Destroy', network, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New network', new_network_path %>
