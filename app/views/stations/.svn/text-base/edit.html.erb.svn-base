<h1>Editing station</h1>

<% form_for(@station) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :dtv %><br />
    <%= f.text_field :dtv %>
  </p>
  <p>
    <%= f.label :antennatype %><br />
    <%= f.text_field :antennatype %>
  </p>
  <p>
    <%= f.label :call_sign %><br />
