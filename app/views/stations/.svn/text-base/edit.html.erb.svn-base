  <p>
    <%= f.label :livedate %><br />
    <%= f.text_field :livedate %>
  </p>
  <p>
    <%= f.label :compassheading %><br />
    <%= f.text_field :compassheading %>
  </p>
  <p>
    <%= f.label :milesfrom %><br />
