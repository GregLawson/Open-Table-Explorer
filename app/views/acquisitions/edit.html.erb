<h1>Editing acquisition</h1>

<% form_for(@acquisition) do |f| %>
  %= f.error_messages %

  <p>
    <%= f.label :acquisition_data %><br />
    <%= f.text_field :acquisition_data %>
  </p>
  <p>
    <%= f.label :created_at %><br />
    <%= f.datetime_select :created_at %>
  </p>
  <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :acquisition_stream_id %><br />
    <%= f.text_field :acquisition_stream_id %>
  </p>
  <p>
    <%= f.label :error %><br />
    <%= f.text_field :error %>
  </p>
  <p>
    <%= f.label :acquisition_updated %><br />
    <%= f.check_box :acquisition_updated %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @acquisition %> |
<%= link_to 'Back', acquisitions_path %>

<p>
  <b>Id:</b>
  <%=h @acquisition_interface.id %>
</p>

<p>
  <b>Name:</b>
  <%=h @acquisition_interface.name %>
</p>

<p>
  <b>Sceme:</b>
  <%=h @acquisition_interface.scheme %>
</p>

