<h1>Listing stream_links</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Input stream method argument</th>
    <th>Output stream method argument</th>
    <th>Storage method</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stream_links.each do |stream_link| %>
  <tr>
    <td><%= stream_link.id %></td>
    <td><%= stream_link.input_stream_method_argument_id %></td>
    <td><%= stream_link.output_stream_method_argument_id %></td>
    <td><%= stream_link.store_method_id %></td>
    <td><%= link_to 'Show', stream_link %></td>
    <td><%= link_to 'Edit', edit_stream_link_path(stream_link) %></td>
    <td><%= link_to 'Destroy', stream_link, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stream link', new_stream_link_path %>
