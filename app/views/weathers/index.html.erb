    <td><%=h weather.klax_pressure_mb %></td>
    <td><%=h weather.klax_pressure_in %></td>
    <td><%=h weather.klax_dewpoint_string %></td>
    <td><%=h weather.klax_dewpoint_f %></td>
    <td><%=h weather.klax_dewpoint_c %></td>
    <td><%=h weather.klax_visibility_mi %></td>
    <td><%= link_to 'Show', weather %></td>
    <td><%= link_to 'Edit', edit_weather_path(weather) %></td>
    <td><%= link_to 'Destroy', weather, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
