<h1>New measurement</h1>

<% form_for(@measurement) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :load_measurement %><br />
    <%= f.text_field :load_measurement %>
  </p>
  <p>
    <%= f.label :load %><br />
    <%= f.text_field :load %>
  </p>
  <p>
    <%= f.label :power_factor %><br />
    <%= f.text_field :power_factor %>
  </p>
  <p>
    <%= f.label :period %><br />
    <%= f.text_field :period %>
  </p>
  <p>
    <%= f.label :duty_cycle_measurement %><br />
    <%= f.text_field :duty_cycle_measurement %>
  </p>
  <p>
    <%= f.label :duty_cycle %><br />
    <%= f.text_field :duty_cycle %>
  </p>
  <p>
    <%= f.label :load_id %><br />
    <%= f.text_field :load_id %>
  </p>
  <p>
    <%= f.label :mode %><br />
    <%= f.text_field :mode %>
  </p>
  <p>
    <%= f.label :va %><br />
    <%= f.text_field :va %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', measurements_path %>
